<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alfaiz.app.newyou">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewYou">
        <activity android:name=".activity.pencegahan.ActivityDetailPencegahan"></activity>
        <activity android:name=".activity.gejala.ActivityDetailGejala" />
        <activity android:name=".activity.pencegahan.ActivityPencegahan" />
        <activity android:name=".activity.gejala.ActivityGejala" />
        <activity android:name=".activity.ActivitySelengkapnya" />
        <activity android:name=".activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>